{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "1.0.0",
    "description": "A basic example extension with only required keys",
    "background": {
        "service_worker": "static/js/service-worker.js",
        "type": "module"
    },
    "action": {
        "default_popup": "static/index.html"
    },
    "permissions": [
        "scripting"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ]
}